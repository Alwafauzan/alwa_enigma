    "def mergesort(arr):\n",
    "    if len(arr) > 1:\n",
    "        r = len(arr)//2\n",
    "        left = arr[:r]\n",
    "        right = arr[r:]\n",
    "        mergesort(left)\n",
    "        mergesort(right)\n",
    "\n"
   ]
